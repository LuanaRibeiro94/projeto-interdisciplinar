{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
      "react"
  ],
  "rules": {
    "react/jsx-filename-extension": "off",
    "arrow-body-style": "off",
    "arrow-parens": "off",
    "global-require": "off",
    "no-use-before-define": "off",
    "react/prop-types": ["error", { "ignore": ["navigation"] }],
    "react/forbid-prop-types": "off"
  },
  "globals": { "fetch": true }
}
